{
    "compilerOptions": {
      "target": "ES2021",
      "module": "NodeNext",
      "moduleResolution": "nodenext",
      "resolveJsonModule": true,
      "baseUrl": "./src",
      "paths": {
        "src/*" : ["*"]
      },
      "outDir": "./build",
      "sourceMap": true,
      "strict": true,
      "esModuleInterop": true
    },
    "include": ["src/**/*", "src/models/.ts"]
}  